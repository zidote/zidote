{
  "editor.defaultFormatter": "trunk.io",
  "editor.formatOnSave": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  "files.associations": {
    "*.json5": "json5"
  },
  "material-icon-theme.folders.associations": {
    ".taskfiles": "utils"
  }
}
